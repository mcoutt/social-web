{
   "type": "postgres",
   "synchronize": true,
   "logging": false,
   "entities": [
      "./src/entity/**/*.ts"
   ],
   "migrations": [
      "./src/migration/**/*.ts"
   ],
   "subscribers": [
      "./src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "./src/db/entity",
      "migrationsDir": "./src/db/migration",
      "subscribersDir": "./src/db/subscriber"
   }
}